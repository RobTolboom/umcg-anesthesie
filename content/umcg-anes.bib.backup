@article{Struys16,
  author = {Michel M R F Struys and Tom De Smet and John Iain B Glen and Hugo E M Vereecke and Anthony R Absalom and Thomas W Schnider},
  title = {The History of Target-Controlled Infusion},
  journal = {Anesth Analg},
  year = {2016},
  volume = {122},
  number = {1},
