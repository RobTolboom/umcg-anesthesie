}

@article{Cankar23,
  author = {Katarina Cankar and Johanna Christina Hakkert and Robert Sevenier and Christina Papastolopoulou and Bert Schipper and João P Baixinho and Naiara Fernández and Melanie S Matos and Ana Teresa Serra and Claudia Nunes Santos and Khabat Vahabi and Alain Tissier and Paul Bundock and Dirk Bosch},
  title = {Lactucin Synthase Inactivation Boosts the Accumulation of Anti-inflammatory 8-Deoxylactucin and Its Derivatives in Chicory (},
