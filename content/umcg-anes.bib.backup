@article{Berg21a,
  author = {Tamar A J van den Berg and Marius C van den Heuvel and Janneke Wiersema-Buist and Jelle Adelmeijer and Gertrude J Nieuwenhuijs-Moeke and Ton Lisman and Stephan J L Bakker and Harry van Goor and Robert A Pol},
  title = {Aggravation of fibrin deposition and microthrombus formation within the graft during kidney transplantation},
  journal = {Sci Rep},
  year = {2021},
  volume = {11},
