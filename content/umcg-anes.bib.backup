@article{Barends23,
  author = {Clemens Barends and Izaak den Daas and Mendy Driesens and Anita Visser and Anthony Absalom and Pieter Colin},
  title = {Development of a pharmacokinetic and pharmacodynamic model for intranasal administration of midazolam in older adults: a single-site two-period crossover study},
  journal = {Br J Anaesth},
  year = {2023},
  volume = {131},
  number = {2},
  pages = {284-293},
  month = {8},
  doi = {10.1016/j.bja.2023.04.035},
  pmid = {37268446},
  abstract = {Intranasal midazolam can produce procedural sedation in frail older patients with dementia who are unable to tolerate necessary medical or dental procedures during domiciliary medical care. Little is known about the pharmacokinetics and pharmacodynamics of intranasal midazolam in older (>65 yr old) people. The aim of this study was to understand the pharmacokinetic/pharmacodynamic properties of intranasal midazolam in older people with the primary goal of developing a pharmacokinetic/pharmacodynamic model to facilitate safer domiciliary sedation care. We recruited 12 volunteers: ASA physical status 1-2, aged 65-80 yr, and received midazolam 5 mg intravenously and 5 mg intranasally on two study days separated by a 6 day washout period. Concentrations of venous midazolam and 1'-OH-midazolam, Modified Observer's Assessment of Alertness/Sedation (MOAA/S) score, bispectral index (BIS), arterial pressure, ECG, and respiratory parameters were measured for 10 h. Time to peak effect of intranasal midazolam for BIS, MAP, and SpO Intranasal bioavailability was high and sedation onset was rapid, with maximum sedative effects after 32 min. We developed a pharmacokinetic/pharmacodynamic model for intranasal midazolam for older persons and an online tool to simulate changes in MOAA/S, BIS, MAP, and SpO EudraCT (2019-004806-90).},
  citations = {6},
}

