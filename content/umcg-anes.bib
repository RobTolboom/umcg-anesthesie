  author = {Lina Vogt and Michelle Schmidt and Andreas Follmann and Andrea Lenes and Martin Klasen and Saša Sopka},
  title = {Telemedicine in medical education: An example of a digital preparatory course for the clinical traineeship - a pre-post comparison},
