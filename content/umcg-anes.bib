  title = {The origin and structures of dimeric fatty acids from the anaerobic reaction between soya-bean lipoxygenase, linoleic acid and its hydroperoxide},
  journal = {Biochem J},
  year = {1972},
  volume = {130},
  number = {2},
  pages = {435-42},
  month = {11},
