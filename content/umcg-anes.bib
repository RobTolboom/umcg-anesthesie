@article{Paul24,
  author = {Matthias Paul and Arnold Eggerschwiler and Gesine Meyer and Christian Studer and Urs Hürlimann and Michael Brändle and Peter Wiesli and Giacomo Gastaldi and Mattia Arrigo and Philippe Meyer and Christian Mueller and Kathrin Zimmermann and Roger Lehmann},
