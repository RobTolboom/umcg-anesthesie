  optnote = {DIAG, RADIOLOGY},
}

@article{Abeysinghe25,
  author = {Pevindu Abeysinghe and Natalie Turner and Holly Flay and Susanne Meier and Leila Cuttle and Murray D Mitchell},
  title = {MicroRNA (miRNA) in Plasma Small Extracellular Vesicles (sEV) as Potential Early Indicators of Dairy Cow Subfertility},
  journal = {J Extracell Biol},
  year = {2025},
