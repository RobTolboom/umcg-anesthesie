  doi = {10.1016/j.bja.2023.11.032},
  pmid = {38114354},
  abstract = {The central and peripheral nervous systems are the primary target organs during anaesthesia. At the time of the inception of the British Journal of Anaesthesia, monitoring of the central nervous system comprised clinical observation, which provided only limited information. During the 100 yr since then, and particularly in the past few decades, significant progress has been made, providing anaesthetists with tools to obtain real-time assessments of cerebral neurophysiology during surgical procedures. In this narrative review article, we discuss the rationale and uses of electroencephalography, evoked potentials, near-infrared spectroscopy, and transcranial Doppler ultrasonography for intraoperative monitoring of the central and peripheral nervous systems.},
}

@article{Welling19,
  author = {Jorrit B A Welling and Karin Klooster and Jorine E Hartman and Huib A M Kerstjens and Ina Franz and Michel M R F Struys and Anthony R Absalom and Dirk-Jan Slebos and Clemens R M Barends},
  title = {Collateral Ventilation Measurement Using Chartis: Procedural Sedation vs General Anesthesia},
