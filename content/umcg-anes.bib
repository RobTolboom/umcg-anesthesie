@article{Keijzer24,
  author = {Ilonka N de Keijzer and Jaap Jan Vos and David Yates and Caroline Reynolds and Sally Moore and Rebecca J Lawton and Thomas W L Scheeren and Simon J Davies},
  title = {Impact of clinicians' behavior, an educational intervention with mandated blood pressure and the hypotension prediction index software on intraoperative hypotension: a mixed methods study},
