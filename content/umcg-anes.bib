@article{Trang25,
  author = {Vo Thi Dieu Trang and Maria Dalgaard Mikkelsen and Monica Daugbjerg Christensen and Sebastian Meier and Cameron James Hunt and Jesper Holck and Guðmundur Óli Hreggviðsson and Jona Freysdottir and Hang Thi Thuy Cao and Huynh Hoang Nhu Khanh and Anne S Meyer},
  title = {A cold-adapted endo-fucoidanase Psf1 from Pseudoalteromonas sp. that catalyzes production of T-cell activating fucoidan oligosaccharides from Saccharina latissima fucoidan},
  journal = {Int J Biol Macromol},
  year = {2025},
