  author = {Morgane Vine and Chantal Daucourt and Cyril Besson and Daria Neyroud and Bengt Kayser and Philippe Meyer and Guillaume Graf and Vincent Gabus and Vincent Gremeaux and Olivier Muller and Aaron L Baggish},
