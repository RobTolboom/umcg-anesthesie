@article{Yeh05,
  author = {Lily Yeh and Bih-Ching Shu},
  title = {[Reflections on the development of indigenous nursing knowledge: an example from psychiatric mental health nursing]},
  journal = {Hu Li Za Zhi},
  year = {2005},
  volume = {52},
  number = {1},
  pages = {18-24},
  month = {2},
  pmid = {15712054},
  abstract = {An awareness of issues concerning the indigenous population has been present in the social and behavioral sciences in Taiwan since 1980. This paper attempts to make critical analyses and reflections on the development of current nursing knowledge in Taiwan. The needs and directions for nursing studies rooted in indigenous culture are discussed in detail.},
}

@article{Yeh99,
  author = {L Yeh and R Pedowitz and S Kwak and P Haghighi and C Muhle and D Trudell and D Resnick},
  title = {Intracapsular origin of the long head of the biceps tendon},
  journal = {Skeletal Radiol},
  year = {1999},
  volume = {28},
  number = {3},
  pages = {178-81},
  month = {3},
  doi = {10.1007/s002560050497},
  pmid = {10231918},
  abstract = {A developmental anomaly of the long head of the biceps tendon was found in a cadaveric shoulder. Findings on arthroscopy, routine MR imaging, and MR arthrography were compared and correlated with results of anatomic dissection. MR arthrography appears to be a very good diagnostic imaging method for depicting this anomaly prior to arthroscopy.},
}

