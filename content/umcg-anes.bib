  title = {Clinical Prognosis of Right-Sided Infective Endocarditis not Associated with Cardiac Devices or Intravenous Drug use: a Cohort Study and Meta-Analysis},
  journal = {Sci Rep},
  year = {2020},
  volume = {10},
  number = {1},
  pages = {7179},
