@article{Warthegau24,
  author = {Stefan S Warthegau and Sebastian Meier},
  title = {Tailoring the Formation of Functionalized Furans from Glucose in Water with Nature-Sourced Catalysts and In Situ NMR},
  journal = {Molecules},
