@article{Barbeck25,
  author = {Mike Barbeck and Kerstin Huebner and Kim Burkhardt and Tonya Andreeva and Rumen Krastev and Reiner Schnettler and Sanja Stojanovic and Stevo Najman and Ole Jung and Regine Schneider-Stock},
  title = {Validation of the Chick Chorioallantoic Membrane (CAM) Model for Biocompatibility Analysis of Biomaterials in the Context of the 3R-cascade: A Pilot Study},
  journal = {In Vivo},
  year = {2025},
  volume = {39},
