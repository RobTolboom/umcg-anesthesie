@article{Jochriem19,
  author = {Markus Jochriem and Dominik Bosch and Holger Kopacka and Benno Bildstein},
  title = {Direct Amination of Cobaltocenium Hexafluoridophosphate via Vicarious Nucleophilic Substitution},
