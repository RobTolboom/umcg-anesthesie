}

@article{Meeks19,
  author = {Karlijn A C Meeks and Peter Henneman and Andrea Venema and Juliet Addo and Silver Bahendeka and Tom Burr and Ina Danquah and Cecilia Galbete and Marcel M A M Mannens and Frank P Mockenhaupt and Ellis Owusu-Dabo and Charles N Rotimi and Matthias B Schulze and Liam Smeeth and Joachim Spranger and Mohammad H Zafarmand and Adebowale Adeyemo and Charles Agyemang},
