  number = {23},
  pages = {2639-40},
  month = {12},
  pmid = {7431612},
  abstract = {Epiglottitis occurs more often in adults than is generally recognized. Six adults with epiglottitis were treated at Evanston (III) Hospital from March to August 1978. Two required tracheostomy because of delayed diagnosis. There were no deaths. This experience suggests that any patient with acute, painful dysphagia should have indirect laryngoscopy or a carefully interpreted lateral roentgenogram to the neck to rule out epiglottitis.},
}

@article{Wolff92,
