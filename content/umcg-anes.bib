}

@article{Kk25,
  author = {Hüray Ilayda Kök and Tonya Andreeva and Sebastian Stammkötter and Cindy Reinholdt and Osman Akbas and Anne Jahn and Florian Gamon and Sandra Fuest and Mirko Teschke and Miriam Schäfer and Michael Müller and Alexander Koch and Ole Jung and Mike Barbeck and Andreas Greuling and Ralf Smeets and Jörg Hermsdorf and Rumen Krastev and Philipp Junker and Meike Stiesch and Frank Walther},
  title = {Characterization and modeling of additively manufactured Ti-6Al-4V alloy with modified surfaces for medical applications},
