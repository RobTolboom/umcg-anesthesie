  doi = {10.1016/j.chest.2019.07.025},
  pmid = {31421111},
  abstract = {Absence of interlobar collateral ventilation is key to successful endobronchial valve treatment in patients with severe emphysema and can be functionally assessed by using the Chartis measurement. This system has been validated during spontaneous breathing, undergoing procedural sedation (PS), but can also be performed under general anesthesia. Performing the Chartis measurement under PS is often challenging because of coughing, mucus secretion, and difficulties in maintaining an adequate level of sedation. The objective of this study was to investigate whether there is a difference in Chartis measurement outcomes between PS and general anesthesia. In this prospective study, patients underwent Chartis measurements under both PS and general anesthesia. Study outcomes were Chartis measurement duration, number of measurements, feasibility, and success rate. The study included 30 patients with severe emphysema (mean age, 62 years; median FEV This study found that Chartis measurement under general anesthesia is faster and more feasible to perform compared with performance with PS, without affecting measurement outcomes. Clinicaltrials.gov; No. NCT03205826; URL: www.clinicaltrials.gov.},
}

@article{Muhle99,
  author = {C Muhle and J M Ahn and L Yeh and G A Bergman and R D Boutin and M Schweitzer and J A Jacobson and P Haghighi and D J Trudell and D Resnick},
